{
  "name": "exbox",
  "version": "0.0.0",
  "description": "A reusable, pre-packaged Vagrant box for developing Elixir applications",
  "bin": "exbox.js",
  "files": [
    "scripts",
    "*.js"
  ],
  "license": "MIT",
  "repository": "lukeed/ExBox",
  "scripts": {
    "test": "xo && nyc ava"
  },
  "author": {
    "name": "Luke Edwards",
    "email": "luke@lukeed.com",
    "url": "https://lukeed.com"
  },
  "keywords": [
    "box",
    "cli-app",
    "cli",
    "config",
    "development",
    "elixir",
    "erlang",
    "vagrant",
    "virtualbox",
    "vm"
  ],
  "dependencies": {
    "commander": "^2.9.0",
    "debug": "^2.2.0",
    "osenv": "^0.1.3",
    "read-pkg": "^1.1.0",
    "update-notifier": "^0.6.3"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "eslint-plugin-node": "^1.1.0",
    "execa": "^0.4.0",
    "nyc": "^6.4.0",
    "xo": "^0.14.0"
  },
  "engines": {
    "node": ">=0.10"
  },
  "xo": {
    "plugins": ["node"],
    "rules": {
      "node/no-unsupported-features": [2, {"version": 0.10, "ignores": ["blockScopedFunctions"]}]
    },
    "overrides": [{
      "files": "test/*.js",
      "rules": {"node/no-unsupported-features": 0}
    }]
  }
}
